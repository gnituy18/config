{
  "manifest_version": 3,
  "name": "Website Confirm",
  "version": "1.0.0",
  "description": "Confirm before entering the website",
  "content_scripts": [
    {
      "matches": [
        "https://x.com/*",
        "https://www.youtube.com/*",
        "https://www.facebook.com/*",
        "https://www.instgram.com/*",
        "https://www.threads.com/*"
      ],
      "js": [
        "website-confirm.js"
      ]
    }
  ]
}
